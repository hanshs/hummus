{
  "name": "@hummus/db",
  "author": "Hans Starkopf",
  "version": "0.0.1",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "clean": "rimraf .turbo node_modules --impl=native",
    "db:gen": "yarn with-env prisma generate",
    "db:push": "yarn with-env prisma db push --skip-generate",
    "db:seed": "yarn with-env prisma db seed",
    "dev": "yarn with-env prisma studio --port 5556",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@types/bcrypt": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "bcrypt": "^5.1.0",
    "dotenv-cli": "^6.0.0",
    "prisma": "^4.8.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  },
  "prisma": {
    "seed": "tsx ./seed.ts"
  }
}
